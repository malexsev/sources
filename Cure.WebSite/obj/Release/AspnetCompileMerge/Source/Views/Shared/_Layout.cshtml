
<!DOCTYPE html>
<html lang="ru" prefix="og: http://ogp.me/ns#">
<head>
    <title>@ViewBag.Title - Наши дети. Лечение ДЦП в Китае</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="robots" content="index, follow">
    <meta name="viewport" content="width=device-width">
    <meta name="keywords" content="лечение ДЦП, лечение ДЦП в Китае">
    <meta name="description" content="Реабилитация и лечение ДЦП в городе Юньчэн Китай | Лечения ДЦП, аутизма, задержка речи, в городе Юньчэн, Китай">
    <meta property="og:url" content="http://mysite.com">
    <meta property="og:type" content="website">
    <meta property="og:title" content="Наши дети. Лечение ДЦП в Китае">
    <meta property="og:image" content="http://mysite.com/logo_big.png">
    <meta property="og:site_name" content="Наши дети. Лечение ДЦП в Китае">
    <meta property="og:description" content="Реабилитация и лечение ДЦП в городе Юньчэн Китай | Лечения ДЦП, аутизма, задержка речи, в городе Юньчэн, Китай">
    <link rel="icon" type="img/png" href="~/content/img/favicon-16x16.png" sizes="16x16" />
    <link rel="icon" type="img/png" href="~/content/img/favicon-32x32.png" sizes="32x32" />
    <link rel="apple-touch-icon-precomposed" href="~/content/img/apple-touch-icon-152x152.png" sizes="152x152" />
    <link rel="stylesheet" href="~/content/css/styles.css" />
    <!--[if lte IE 9]>
      <script src="~/Scripts/vendor/modernizr-2.8.3.min.js"></script>
    <![endif]-->
</head>

<body>
    <!--[if lte IE 9]>
      <div class="stop-browser-modal">
        Вы используете очень <strong>устаревший</strong> браузер, котороый не<br>
        обеспечивает безошибочную работу современных сайтов.<br> Пожалуйста,
        <a href="http://browsehappy.com/">обновите ваш браузер</a><br>
        для получения возможности полноценно пользоваться интернетом.
      </div>
    <![endif]-->
    <noscript>
        <div class="stop-browser-modal">
            <strong>JavaScript seems to be disabled in your browser.</strong><br>
            You must have JavaScript enabled in your browser to utilize the functionality of this website.
        </div>
    </noscript>

    <div class="preloader">
        <img src="~/content/img/preloader.gif" alt="Preloader">
    </div>

    <div class="wrapper account">
        <header class="header">
            <div class="header-topline">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <ul class="header-topline-btns">
                                @if (!User.Identity.IsAuthenticated)
                                {
                                    <li><a class="header-btn js-show-popup" data-pop="pop-registr"><i class="ico-login"></i>Вход/регистрация</a></li>
                                }
                                <li><a class="header-btn"><i class="ico-help"></i>Помощь</a></li>
                                <li><a class="header-btn"><i class="ico-search"></i>Поиск</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <a class="header-mobbtn js-toggle-menu"><span class="lines"></span></a>
                        <div class="header-logo">
                            <a href="@Url.Action("Index", "Home")" title="Наша клиника: основной сайт">
                                <img src="~/Content/img/logo_long.png" alt="Наша клиника">
                            </a>
                        </div>
                        <div class="header-btns">
                            <a class="btn btn-orang-outlined" href="@Html.Raw(User.Identity.IsAuthenticated ? Url.Action("Orders", "Cabinet") : Url.Action("Registration", "Preorder"))">ЗАПИСАТЬСЯ НА ЛЕЧЕНИЕ</a>
                        </div>
                        @Html.Partial("_MainMenu")
                    </div>
                </div>
            </div>

            @Html.Partial("_MainMenuExt")

            @if (User.Identity.IsAuthenticated)
            {
                @Html.Partial("_AccountMenu")
            }
        </header>

        @RenderBody()

    </div><!-- END main-wrapper -->
    @Html.Partial("_Footer")

    @Html.Partial("_Account")

    @if (Request.Url.AbsolutePath.ToLower().Contains("mypage"))
    {
        @Html.Partial("_CabinetChildEdit")
    }

    @if (Request.Url.AbsolutePath.ToLower().Contains("mension"))
    {
        @Html.Partial("_MensionAddPopup")    ;
    }

    @if (Request.Url.AbsolutePath.ToLower().Contains("department") && Request.Url.AbsolutePath.ToLower().Contains("sevastopol"))
    {
        @Html.Partial("_SevastopolPops")    ;
    }

    @if (Request.Url.AbsolutePath.ToLower().Contains("department") && Request.Url.AbsolutePath.ToLower().Contains("almaata"))
    {
        @Html.Partial("_AlmaataPops")    ;
    }

    @if (Request.Url.AbsolutePath.ToLower().Contains("department") && Request.Url.AbsolutePath.ToLower().Contains("yancheng1"))
    {
        @Html.Partial("_Yancheng1Pops")    ;
    }

    @if (Request.Url.AbsolutePath.ToLower().Contains("department") && Request.Url.AbsolutePath.ToLower().Contains("yancheng2"))
    {
        @Html.Partial("_Yancheng2Pops");
    }


    <!-- jquery cdn        -->
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <!-- if no google cdn  -->
    <script>window.jQuery || document.write('<script src="~/Scripts/vendor/jquery-1.11.3.min.js"><\/script>');</script>
    <!-- content slider    -->
    <script src="~/Scripts/plugins/slick.min.js"></script>
    <!-- numbers odometer  -->
    <script src="~/Scripts/plugins/odometer.min.js"></script>
    <!-- datepicker        -->
    <script src="~/Scripts/plugins/bootstrap-datepicker.min.js"></script>
    <!-- datepicker ru     -->
    <script src="~/Scripts/plugins/bootstrap-datepicker.ru.min.js"></script>
    <!-- viewport checker  -->
    <script src="~/Scripts/plugins/jquery.viewportchecker.min.js"></script>
    <!-- form input masks  -->
    <script src="~/Scripts/plugins/jquery.maskedinput.min.js"></script>
    <!-- form validate     -->
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <!-- main scripts      -->
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <!-- main scripts      -->
    <script src="~/Scripts/jquery.ndeti.js"></script>
    <!-- zoom images       -->
    <script src="~/Scripts/plugins/transition.js"></script>
    <script src="~/Scripts/plugins/zoom.min.js"></script>
    <!-- scroll effects    -->
    <script src="~/Scripts/plugins/jquery.scroolly.min.js"></script>
    <!-- pop-up plugin  -  -->
    <script src="~/Scripts/plugins/jquery.swipebox.min.js"></script>
    <script type="text/javascript" src="~/Scripts/plugins/gismeteo.js"></script>

</body>
</html>
